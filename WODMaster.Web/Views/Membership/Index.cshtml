@model List<WODMaster.BLL.DTO.MembershipModel>;

@{
    ViewData["Title"] = "Index";
}

<h1>Gestion de Membresias</h1>
<br />
<br />
<h2>Perfil de usuarios</h2>
<br />
<br />

@foreach (var usuarios in Model)
{
    <div class="container text-center">
        <div class="row">
            <div class="col">
                @usuarios.Name: @usuarios.Description
            </div>
            <div class="col">
                @usuarios.Price
            </div>
            <div class="col">
                @usuarios.TotalClassesAllowed
            </div>
        </div>
    </div>
}