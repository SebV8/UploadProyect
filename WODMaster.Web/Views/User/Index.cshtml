@model List<WODMaster.BLL.DTO.UserModel>;

@{
    ViewData["Title"] = "Index";
}

<h1>Gestion de Membresias</h1>
<br />
<br />
<h2>Perfil de usuarios</h2>
<br />
<br />

@foreach (var usuarios in Model)
{
    <div class="container text-center">
        <div class="row">
            <div class="col">
                @usuarios.Nombre @usuarios.Apellido
            </div>
            <div class="col">
                @usuarios.Correo
            </div>
            <div class="col">
                @usuarios.Telefono
            </div>
        </div>
    </div>
}