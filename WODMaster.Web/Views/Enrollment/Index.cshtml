@model List<WODMaster.BLL.DTO.EnrollmentModel>;

@{
    ViewData["Title"] = "Index";
}

<h1>Gestion de Membresias</h1>
<br />
<br />
<h2>Perfil de usuarios</h2>
<br />
<br />

@foreach (var usuarios in Model)
{
    <div class="container text-center">
        <div class="row">
            <div class="col">
                @usuarios.Id: @usuarios.MembershipId
            </div>
            <div class="col">
                @usuarios.UserId
            </div>
            <div class="col">
                @usuarios.StartDate
            </div>
        </div>
    </div>
}